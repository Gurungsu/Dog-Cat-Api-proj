






#progOutput tbody img{
  max-width: 300px;
}


#progOutput table{
  overflow: auto;;
  height: 1000px;
  border-style: double;
  border-color:greenyellow;
}

#progOutput td{
  border-style: solid;
  overflow: auto;
}
#topContainer {
  border: 4px solid rgb(0, 255, 64);
}

#pageTitle {
  text-align: left;
}

#userInput {
  background-color: rgb(87, 143, 112);
  text-align: center;
}

#progOutput {
  background-color: rgba(109, 177, 156, 0.952);
}
#topContainer {
  display: grid;
  grid-template-columns: 8fr 3fr;
  grid-template-areas: 
    "header header"
    "catdata control";
}
#pageTitle {
  grid-area: header;
}

#progOutput {
  grid-area: catdata;
  grid-template-columns: 2fr 2fr;
}


